    <script>

        $(document).foundation();
        $(document).ready(function(){$('#myModal').foundation('reveal', 'open')});
        $(document).ready(function() {

            $(".location").click(function() {
                $(".hide").hide();
                var dataType = $(this).attr('data-type');
                $("#" + dataType).show();
            });

            // $("#openChow a").click(function() {
            //     $("#chowNow").show();
            //     $("#openChow").hide();
            //     document.getElementById("chownow-iframe").src="https://facebook.chownow.com/search/locationPicker?company_id=2050";
            // });

            setTimeout(function() {
                $('.map-nav a:first-child').addClass('location_trig');
                $('.map-nav a:first-child').trigger('click');
            },2);
        });

        $( "a.location" ).hover(
            function() {
                $( this ).addClass( "location_roll" );
            }, function() {
                $( this ).removeClass( "location_roll" );
            }
            );
        $( ".map-nav a:nth-child(1)" ).click(
            function() {
                $( "a.location" ).removeClass( "location_trig" );
                $( this ).addClass( "location_trig" );
            }
            );
        $( ".map-nav a:nth-child(2)" ).click(
            function() {
                $( "a.location" ).removeClass( "location_trig" );
                $( this ).addClass( "location_trig" );
            }
            );
        $( ".map-nav a:nth-child(3)" ).click(
            function() {
                $( "a.location" ).removeClass( "location_trig" );
                $( this ).addClass( "location_trig" );
            }
            );
        $( ".map-nav a:nth-child(4)" ).click(
            function() {
                $( "a.location" ).removeClass( "location_trig" );
                $( this ).addClass( "location_trig" );
            }
            );
        $( ".map-nav a:nth-child(5)" ).click(
            function() {
                $( "a.location" ).removeClass( "location_trig" );
                $( this ).addClass( "location_trig" );
            }
            );

    </script>
